{
    "compilerOptions": {
      "resolveJsonModule": true,
      "module": "ESNext",
      "target": "ES2020",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      //"types": ["@playwright/test", "node"],
      "types": ["node"],
      "typeRoots": ["node_modules/@types"]
    },
}